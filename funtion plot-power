import numpy as np
import matplotlib.pyplot as plt

n = int(input('input:'))
function_range = list(input('the range of plot:'))
t = np.arange(int(function_range[0]), 
              int(function_range[2]), 0.01)
plt.plot(t,np.power(t,n))
